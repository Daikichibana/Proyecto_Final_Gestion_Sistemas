@*@page "/RegistrarResponsableAlmacen"

<h3>Registrar Responsable de Almacen</h3>

<!-- Formulario Registro -->
<div style="display: flex; justify-content: center;">
    <form style="width: 70%;">
        <!-- Informacion del responsable -->

        <fieldset class="form-group">
            <legend>Registrar Sucursal</legend>
            <div class="form-group">
                <label for="NombreRubro" class="form-label mt-4">Nombre sucursal</label>
                <input type="text" class="form-control" id="NombreRubro" placeholder="Ingrese nombre de sucursal.">
            </div>
            <div class="form-group">
                <label for="NombreRubro" class="form-label mt-4">Direccion</label>
                <input type="text" class="form-control" id="NombreRubro" placeholder="Ingrese direccion de sucursal.">
            </div>
            <div class="form-group">
                <label for="NombreRubro" class="form-label mt-4">Telefono</label>
                <input type="text" class="form-control" id="NombreRubro" placeholder="Ingrese telefono de sucursal.">
            </div>
            <div class="form-group">
                <label for="NombreRubro" class="form-label mt-4">Correo</label>
                <input type="text" class="form-control" id="NombreRubro" placeholder="Ingrese correo de sucursal.">
            </div>

        </fieldset>
        <!-- Boton enviar -->
    </form>
</div>

<!-- Boton enviar -->

<div style="display: flex; justify-content: center; align-items: center; height: 250px;">
    <button type="submit" class="btn btn-primary" id="btnVolver" style="font-size: 20px; margin-right: 20px">Volver atras</button>
    <button type="submit" class="btn btn-primary" id="enviarFormulario" style="font-size: 20px;">Registrar</button>
</div>


